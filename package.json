{
  "name": "vue-simple-hotkey",
  "version": "0.1.2",
  "description": "Simple hotkey plugin for vue.js",
  "author": "Hyun Woo Park (phu54321@naver.com)",
  "license": "MIT",
  "lint-staged": {
    "linters": {
      "*.{js,vue}": [
        "eslint --fix",
        "git add"
      ]
    },
    "ignore": [
      "**/*.min.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && karma start --single-run"
    }
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:webpack": "webpack --config webpack/prod --watch",
    "dev:karma": "karma start",
    "build": "webpack --config webpack/prod --progress --hide-modules",
    "prepare": "npm run build",
    "test": "karma start"
  },
  "main": "dist/index.js",
  "dependencies": {},
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.25",
    "@babel/polyfill": "^7.0.0",
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.1.3",
    "jquery": "^3.3.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "lint-staged": "^8.0.4",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "vue": "^2.1.0",
    "vue-loader": "^10.0.0",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.1.0",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.9"
  },
  "peerDependencies": {
    "jquery": "^3.3.1",
    "vue": "^2.5.17"
  }
}
